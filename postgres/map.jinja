{% import_yaml "yamls/openstack/postgres.yaml" as openstack %}
{% import_yaml "yamls/nova/postgres.yaml" as nova %}
{% import_yaml "yamls/neutron/postgres.yaml" as neutron %}
{% import_yaml "yamls/keystone/postgres.yaml" as keystone %}
{% import_yaml "yamls/glance/postgres.yaml" as glance %}
{% do openstack.postgres.users.update(neutron.postgres.users) %}
{% do openstack.postgres.users.update(nova.postgres.users) %}
{% do openstack.postgres.users.update(keystone.postgres.users) %}
{% do openstack.postgres.users.update(glance.postgres.users) %}
{% set postgres = openstack['postgres'] %}
